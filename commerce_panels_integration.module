<?php

/**
 *
 * @param type $owner
 * @param type $plugin_type
 * @return type
 */
function commerce_panels_integration_ctools_plugin_directory($owner, $plugin_type) {
  if ($owner == 'page_manager') {
    return 'plugins/' . $plugin_type;
  }
  if ($owner == 'ctools') {
    return 'plugins/' . $plugin_type;
  }
}
